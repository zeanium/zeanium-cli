{
  "name": "application",
  "version": "1.0.1",
  "description": "application.",
  "scripts": {
    "zr.development": "rimraf ./web/www/dist/development && webpack --config ./web/__webpack__/development.js --znui-react.path ../../../../znui/ --resolve.alias.path ../../znui/ --resolve.alias.prefix znui-react",
    "zr.development.watch": "rimraf ./web/www/dist/development && webpack --config ./web/__webpack__/development.js --znui-react.path ../../../../znui/ --resolve.alias.path ../../znui/ --resolve.alias.prefix znui-react --watch",
    "zr.stage": "rimraf ./web/www/dist/stage && webpack --config ./web/__webpack__/stage.js --uglify",
    "zr.stage.watch": "rimraf ./web/www/dist/stage && webpack --config ./web/__webpack__/stage.js --watch",
    "zr.production": "rimraf ./web/www/dist/production && webpack --config ./web/__webpack__/production.js --uglify",
    "zr.production.watch": "rimraf ./web/www/dist/production && webpack --config ./web/__webpack__/production.js --watch",
    "install.release": "npm install --production",
    "install.dev": "npm install"
  },
  "keywords": [],
