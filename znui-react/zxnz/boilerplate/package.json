    "znui-react-button": "latest",
    "znui-react-icon": "latest",
    "znui-react-input": "latest",
    "znui-react-layout": "latest",
    "znui-react-loader": "latest",
    "znui-react-pager": "latest",
    "znui-react-popup": "latest",
    "znui-react-router": "latest",
    "znui-react-selector": "latest",
